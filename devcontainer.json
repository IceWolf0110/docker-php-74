{
  "name": "soicau247",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "settings": {
    "terminal.integrated.defaultProfile.linux": "bash"
  },
  "workspaceFolder": "/workspace",
  "remoteUser": "root",
  "runArgs": [
    "--privileged",
    "--cap-add=SYS_PTRACE",
    "--security-opt",
    "seccomp=unconfined"
  ],
  "forwardPorts": [8000, 3306]
}
